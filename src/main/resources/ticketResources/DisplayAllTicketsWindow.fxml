<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="540.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.greentracker.scene.controllers.ticketControllers.DisplayAllTicketsController">
    <ImageView fx:id="imageView" fitHeight="107.0" fitWidth="229.0" layoutX="370.0" layoutY="14.0" pickOnBounds="true"
               preserveRatio="true">
        <Image url="@../logo.png"/>
    </ImageView>
    <TextArea fx:id="ticketsTextArea" editable="false" layoutX="327.0" layoutY="157.0" prefHeight="278.0"
              prefWidth="286.0"/>
    <Button fx:id="returnButton" layoutX="441.0" layoutY="496.0" mnemonicParsing="false" onAction="#switchToMenuWindow"
            text="Retour">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
</AnchorPane>
