<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="540.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.greentracker.scene.controllers.ticketControllers.DisplayTicketInfoController">
    <ImageView fx:id="imageView" fitHeight="107.0" fitWidth="229.0" layoutX="371.0" layoutY="14.0" pickOnBounds="true"
               preserveRatio="true">
        <Image url="@../logo.png"/>
    </ImageView>
    <Text fx:id="ticketNameText" layoutX="428.0" layoutY="235.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Nom du ticket" textAlignment="CENTER">
        <font>
            <Font size="14.0"/>
        </font>
    </Text>
    <TextField fx:id="ticketNameTextField" layoutX="398.0" layoutY="240.0"/>
    <Button fx:id="validateButton" layoutX="441.0" layoutY="291.0" mnemonicParsing="false" onAction="#validate"
            text="Valider">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <TextArea fx:id="ticketTextArea" editable="false" layoutX="372.0" layoutY="170.0" prefHeight="200.0"
              prefWidth="200.0" visible="false" wrapText="true">
        <font>
            <Font size="14.0"/>
        </font>
    </TextArea>
    <Button fx:id="returnButton" layoutX="443.0" layoutY="496.0" mnemonicParsing="false" onAction="#switchToMenuWindow"
            text="Retour">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
</AnchorPane>
