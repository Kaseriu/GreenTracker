<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="540.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.greentracker.scene.controllers.stateControllers.CreateStateController">
    <ImageView fx:id="imageView" fitHeight="107.0" fitWidth="229.0" layoutX="379.0" pickOnBounds="true"
               preserveRatio="true">
        <Image url="@../logo.png"/>
    </ImageView>
    <Text fx:id="stateNameText" layoutX="431.0" layoutY="217.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Nom de l'état">
        <font>
            <Font size="14.0"/>
        </font>
    </Text>
    <TextField fx:id="stateNameTextField" layoutX="405.0" layoutY="221.0"/>
    <Button fx:id="validateButton" layoutX="449.0" layoutY="273.0" mnemonicParsing="false" onAction="#validate"
            text="Valider">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <Text fx:id="validateText" layoutX="445.0" layoutY="333.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Etat créé"
          textAlignment="CENTER" visible="false" wrappingWidth="70.2939453125">
        <font>
            <Font size="14.0"/>
        </font>
    </Text>
    <Button fx:id="returnButton" layoutX="450.0" layoutY="496.0" mnemonicParsing="false" onAction="#switchToMenuWindow"
            text="Retour">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
</AnchorPane>
