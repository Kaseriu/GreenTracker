<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="540.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.greentracker.scene.controllers.authControllers.InscriptionController">
    <ImageView fx:id="imageView" fitHeight="107.0" fitWidth="229.0" layoutX="380.0" layoutY="14.0" pickOnBounds="true"
               preserveRatio="true">
        <Image url="@../logo.png"/>
    </ImageView>
    <Text fx:id="nameText" layoutX="465.0" layoutY="162.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nom">
        <font>
            <Font size="14.0"/>
        </font>
    </Text>
    <TextField fx:id="nameTextField" layoutX="406.0" layoutY="166.0"/>
    <Text fx:id="emailText" layoutX="463.0" layoutY="233.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email">
        <font>
            <Font size="14.0"/>
        </font>
    </Text>
    <TextField fx:id="emailTextField" layoutX="405.0" layoutY="237.0"/>
    <Text fx:id="passwordText" layoutX="439.0" layoutY="304.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Mot de passe">
        <font>
            <Font size="14.0"/>
        </font>
    </Text>
    <PasswordField fx:id="passwordField" layoutX="405.0" layoutY="308.0"/>
    <Button fx:id="validateButton" layoutX="448.0" layoutY="364.0" mnemonicParsing="false" onAction="#inscription"
            text="Valider">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <Button fx:id="returnButton" layoutX="449.0" layoutY="496.0" mnemonicParsing="false" onAction="#switchToMainWindow"
            text="Retour">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
</AnchorPane>
